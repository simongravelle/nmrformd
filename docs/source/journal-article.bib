@article{gerebenInvestigationStructureEthanol2015,
  title = {Investigation of the {{Structure}} of {{Ethanol}}–{{Water Mixtures}} by {{Molecular Dynamics Simulation I}}: {{Analyses Concerning}} the {{Hydrogen-Bonded Pairs}}},
  shorttitle = {Investigation of the {{Structure}} of {{Ethanol}}–{{Water Mixtures}} by {{Molecular Dynamics Simulation I}}},
