}

@article{klippensteinIntroducingMemoryCoarseGrained2021,
  title = {Introducing {{Memory}} in {{Coarse-Grained Molecular Simulations}}},
  author = {Klippenstein, Viktor and Tripathy, Madhusmita and Jung, Gerhard and Schmid, Friederike and {vanderVegt}, Nico F. A.},
