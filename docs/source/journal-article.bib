@article{amayuelasMechanismWaterIntrusion2023,
  title = {Mechanism of {{Water Intrusion}} into {{Flexible ZIF-8}}: {{Liquid Is Not Vapor}}},
  shorttitle = {Mechanism of {{Water Intrusion}} into {{Flexible ZIF-8}}},
  author = {Amayuelas, Eder and Tortora, Marco and Bartolomé, Luis and Littlefair, Josh David and Paulo, Gonçalo and Le Donne, Andrea and Trump, Benjamin and Yakovenko, Andrey Andreevich and Chorążewski, Mirosław and Giacomello, Alberto and Zajdel, Paweł and Meloni, Simone and Grosu, Yaroslav},
  date = {2023-06-28},
  journaltitle = {Nano Letters},
