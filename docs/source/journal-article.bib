@article{philipsQuadrupolarNMRRelaxation2020,
  title = {Quadrupolar {{NMR Relaxation}} of {{Aqueous}} {\textsuperscript{127}} {{I}} {\textsuperscript{–}} , {\textsuperscript{131}} {{Xe}}, and {\textsuperscript{133}} {{Cs}} {\textsuperscript{+}} : {{A First-Principles Approach}} from {{Dynamics}} to {{Properties}}},
